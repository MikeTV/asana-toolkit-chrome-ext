{
  "name": "Task Toolkit for Asana",
  "version": "0.0.21",
  "manifest_version": 3,
  "description": "Estimate time on your Asana board.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "js": [
        "scripts/contentscript.js"
      ],
      "matches": [
        "https://app.asana.com/*"
      ]
    }
  ],
  "permissions": [
  ],
  "host_permissions": [
    "https://app.asana.com/*"
  ]
}
